#include <stdio.h>

int main() {
    int num1, num2;

    printf("Digite o primeiro número inteiro: ");
    scanf("%d", &num1);
    printf("Digite o segundo número inteiro: ");
    scanf("%d", &num2);

    if (num1 <= num2) {
        for (int i = num1; i <= num2; i++) {
            printf("%d ", i);
        }
    } else {
        for (int i = num1; i >= num2; i--) {
            printf("%d ", i);
        }
    }

    return 0;
}
